public class FizzBuzz{
  
  public static void FizzBuzz(int n){
    boolean test3 = n % 3 == 0;
    boolean test5 = n % 5 == 0;
     
    if (test3 == true) {
      if (test5 == true) {
        System.out.println("FizzBuzz");
      } else {
        System.out.println("Fizz");
      }
    } else if (test5 == true) {
      System.out.println("Buzz");
    }
  }
}
      